
.body{
	border-top: solid 1px #DBDBDA;
	padding: 0 40rpx;
}
/* 头像 */
.face-wapper{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	
	margin: 120rpx 0;
}
.face{
	width: 160rpx;
	height: 160rpx;
	-webkit-border-radius: 50%;
	        border-radius: 50%;
}
/* 注册登陆 */
.info-wapper{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	
	border-bottom: solid 1px #DBDBDa;
	padding-bottom: 20rpx;
}
.words-lbl{
	color: #808080;
}
.input{
	width: 500rpx;
	margin-left: 40rpx;
}
.graywords{
	color: #EAEAEA;
}
/* 第三方登陆的样式设置 */
.third-wapper{
	width: 100%;
	margin-top: 60rpx;
}
.third-words{
	color: #A9A9A9;
	font-size: 13px;
	
	display: -webkit-box;
	
	display: -webkit-flex;
	
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
}
.third-line{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
}
.single-line{
	padding: 15rpx 20rpx;
	width: 25%;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.third-icos-wapper{
	margin-top: 30rpx;
	
	display: -webkit-box;
	
	display: -webkit-flex;
	
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
}
.third-ico{
	width: 60rpx;
	height: 60rpx;
}
.third-btn-ico{
	background-image:url(data:image/png;base64,);
	width: 60rpx;
	height: 60rpx;
	background-color: white;
	background-size: 60rpx 60rpx;
	background-repeat: no-repeat;
	border: none;
	padding: 0;
}
button::after{
	border: none;
}

